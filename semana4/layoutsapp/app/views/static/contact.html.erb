<%= content_for :head do %>
  <title>Contact US</title>
<% end %>

<%= content_for :intro do %>
  <h1>Contact US</h1>
  <h3>Env√≠anos tus datos de contacto</h3>
<% end %>

<%= form_for :upc, :html => {:method => :post}, :url => {:controller => 'static', :action => 'mimetodo'} do |f| %>

  <%= f.label  :titulo %>
  <%= f.text_field  :titulo, :class => 'form-control' %>
  <br >

  <%= f.label  :descripcion %>
  <%= f.text_area  :descripcion, :class => 'form-control' %>
  <br />

  <%= f.submit "Enviar datos", :class => "btn btn-primary" %>


<% end %>
